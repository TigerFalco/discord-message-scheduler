<h1>User#new</h1>
<p>Find me in app/views/user/new.html.erb</p>
<%=form_for :user, url: '/user/create', method: :post do%>
  <div>
    <%= label_tag :username%>
    <%= text_field_tag :username%>
  </div>
  <div>
    <%= label_tag :password%>
    <%= text_field_tag :password%>
  </div>

  <%= submit_tag 'Sign Up', class: "" %>
<%end%>